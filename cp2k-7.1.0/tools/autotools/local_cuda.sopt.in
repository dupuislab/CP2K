CC = @CC@
CXX = @CXX@
CPP = @CPP@
AR = @AR@ -r
FC = @FC@
LD = @FC@
NVCC = @NVCC@
NVFLAGS = @NVCC_FLAGS@ -std=c++11
GPUVER = @GPUVER@

WFLAGS = @WFLAGS@
CFLAGS = @LIBINT2_CFLAGS@ @SOPT_CFLAGS@ @FFTW3_CFLAGS@ @CUDA_CFLAGS@ @XSMM_CFLAGS@ @LIBSMM_CFLAGS@ @BLAS_CFLAGS@ @LIBXC_CFLAGS@
DFLAGS = @LIBINT2_DFLAGS@ @CUDA_DFLAGS@ @FFTW3_DFLAGS@ @LIBINT_DFLAGS@ @XSMM_DFLAGS@ @SMM_DFLAGS@ @ELPA_DFLAGS@
FCFLAGS = @LIBINT2_CFLAGS@ @SOPT_FFLAGS@ @CUDA_CFLAGS@ @FFTW3_CFLAGS@ @LIBXC_CFLAGS@ @BLAS_FFLAGS@ @LIBSMM_FFLAGS@ @LIBXSMM_CFLAGS@ $(WFLAGS)
LDFLAGS = @LIBINT2_LDFLAGS@ @LIBXC_LDFLAGS@ @FFTW3_LDFLAGS@ @BLAS_LDFLAGS@ @LIBSMM_LDFLAGS@ @LIBXSMM_LDFLAGS@
LIBS = @LIBINT2_LIBS@ @LIBXC_LIBS@ @LIBXC_F03_LIBS@ @FFTW3_LIBS@ @BLAS_LIBS@ @CUDA_LIBS@ @LIBSMM_LIBS@ @LIBXSMM_LIBS@ @PTHREAD_LIBS@ -lstdc++
FYPPFLAGS = -n --line-marker-format=gfortran5
